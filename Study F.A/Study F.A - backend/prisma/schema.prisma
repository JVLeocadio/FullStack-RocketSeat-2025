datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "./node_modules/@prisma/client"
}

model Video {
  id          Int      @id @default(autoincrement())
  sessionType String
  season      String
  video_date  DateTime
  path        String
  tags        String
  uploadMonth Int
  uploadDay   Int
}